version: '2'
services: 
  dst:
    image: nginx
    ports:
      - "80:80"
      - "443:443"
    volumes:
      - ./nginx.conf:/etc/nginx/nginx.conf:ro
      - ./http/:/etc/nginx/http:ro
      - ./https/:/etc/nginx/https:ro
      - ./www/:/usr/share/nginx/html:ro
      - /home/githubactions/party_fowl_games/:/www/party_fowl_games:ro
    restart: always
